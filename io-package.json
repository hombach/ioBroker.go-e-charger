{
  "common": {
    "name": "go-e-charger",
    "version": "0.1.0",
    "title": "go-eCharger",
    "titleLang": {
      "en": "go-eCharger",
      "de": "go-eCharger",
      "ru": "gо-eCharger",
      "pt": "go-eCharger",
      "nl": "go-eCharger",
      "fr": "go-eCharger",
      "it": "go-eCharger",
      "es": "go-eCharger",
      "pl": "go-eCharger",
      "zh-cn": "电子充电器"
    },
    "desc": {
      "en": "Adapter for reading go-eCharger data",
      "de": "Adapter zum Lesen von go-eCharger-Daten",
      "ru": "Адаптер для чтения данных go-eCharger",
      "pt": "Adaptador para leitura de dados do go-eCharger",
      "nl": "Adapter voor het lezen van go-eCharger-gegevens",
      "fr": "Adaptateur pour lire les données de go-eCharger",
      "it": "Adattatore per la lettura di dati go-eCharger",
      "es": "Adaptador para leer datos de go-eCharger",
      "pl": "Adapter do odczytu danych go-eCharger",
      "zh-cn": "用于读取go-eCharger数据的适配器"
    },
    "authors": [
      "Christian Hombach"
    ],
    "platform": "Javascript/Node.js",
    "mode": "daemon",
    "enabled": true,
    "license": "MIT",
    "loglevel": "info",
    "readme": "https://github.com/hombach/ioBroker.go-e-charger/blob/master/README.md",
    "icon": "go-eCharger.png",
    "extIcon": "https://github.com/hombach/ioBroker.go-e-charger/blob/master/admin/go-eCharger.png",
    "keywords": [ "go-e", "go-eCharger", "wallbox" ],
    "type": "energy",
    "connectionType": "local",
    "dataSource": "poll",
    "news": {
      "0.1.0": {
        "en": "setup state machine"
      },
      "0.0.9": {
        "en": "read charger power"
      },
      "0.0.8": {
        "en": "setup done"
      },
      "0.0.1": {
        "en": "initial release"
      }
    },
    "main": "main.js",
    "compact": true,
    "materialize": true,
    "materializeTab": true,
    "supportCustoms": false,
    "dependencies": [
      {
        "js-controller": ">=3.0.0",
        "admin": ">=3.0.0"
      }
    ]
  },

  "native": {
    "ipaddress": "",
    "polltimelive": 10000
  },

  "objects": [],

  "instanceObjects": [
    {
      "_id": "Info",
      "type": "channel",
      "common": { "name": "Informations about go-eCharger" },
      "native": {}
    },
    {
      "_id": "Info.RebootCounter",
      "type": "state",
      "common": {
        "role": "value",
        "name": "Counter for system reboot events",
        "type": "number",
        "unit": "",
        "read": true,
        "write": false,
        "def": 0
      },
      "native": {}
    },
    {
      "_id": "Info.RebootTimer",
      "type": "state",
      "common": {
        "role": "value",
        "name": "Time since last reboot",
        "type": "number",
        "unit": "h",
        "read": true,
        "write": false,
        "def": 0
      },
      "native": {}
    },
    {
      "_id": "Info.CarState",
      "type": "state",
      "common": {
        "role": "value",
        "name": "State of connected car",
        "type": "number",
        "unit": "",
        "read": true,
        "write": false,
        "def": 0
      },
      "native": {}
    },


    {
      "_id": "Power",
      "type": "channel",
      "common": { "name": "current charger power data" },
      "native": {}
    },
    {
      "_id": "Power.ChargeCurrent",
      "type": "state",
      "common": {
        "role": "value",
        "name": "Charge current output",
        "type": "number",
        "unit": "A",
        "read": true,
        "write": false,
        "def": 0
      },
      "native": {}
    },
    {
      "_id": "Power.GridPhases",
      "type": "state",
      "common": {
        "role": "value",
        "name": "Flags for primary and secondary grid phases",
        "type": "number",
        "unit": "Flags",
        "read": true,
        "write": false,
        "def": 0
      },
      "native": {}
    },
    {
      "_id": "Power.Charge",
      "type": "state",
      "common": {
        "role": "value",
        "name": "actual charging-power",
        "type": "number",
        "unit": "W",
        "read": true,
        "write": false,
        "def": 0
      },
      "native": {}
    },
    {
      "_id": "Power.ChargingAllowed",
      "type": "state",
      "common": {
        "role": "value",
        "name": "Charging allowed",
        "type": "boolean",
        "unit": "",
        "read": true,
        "write": false,
        "def": false
      },
      "native": {}
    },


    {
      "_id": "Statistics_Total",
      "type": "channel",
      "common": { "name": "statistical data total lifetime" },
      "native": {}
    },
    {
      "_id": "Statistics_Total.Charged",
      "type": "state",
      "common": {
        "role": "value",
        "name": "Totally charged in go-e lifetime",
        "type": "number",
        "unit": "kWh",
        "read": true,
        "write": false,
        "def": 0
      },
      "native": {}
    },


    {
      "_id": "Settings",
      "type": "channel",
      "common": { "name": "states to dynamically adjust go-eCharger settings" },
      "native": {}
    },
    {
      "_id": "Settings.Setpoint_HomeBatSoC",
      "type": "state",
      "common": {
        "role": "value",
        "name": "Setpoint for home battery state of charge",
        "type": "number",
        "unit": "%",
        "read": true,
        "write": true,
        "def": 50
      },
      "native": {}
    },
    {
      "_id": "Settings.ChargeNOW",
      "type": "state",
      "common": {
        "role": "switch",
        "name": "ChargeNOW enabled",
        "type": "boolean",
        "unit": "",
        "read": true,
        "write": true,
        "def": false
      },
      "native": {}
    },
    {
      "_id": "Settings.ChargeManager",
      "type": "state",
      "common": {
        "role": "switch",
        "name": "Charge Manager enabled",
        "type": "boolean",
        "unit": "",
        "read": true,
        "write": true,
        "def": false
      },
      "native": {}
    }
  ]
}
